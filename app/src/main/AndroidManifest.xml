<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.todayrest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/login_logo3"
        android:label="@string/app_name"
        android:roundIcon="@drawable/login_logo3"
        android:supportsRtl="true"
        android:theme="@style/Theme.TodayRest">
        <activity
            android:name=".WorkTime"
            android:exported="false" />
        <activity
            android:name=".SleepTime"
            android:exported="true" >

        </activity>
        <activity
            android:name=".Login"
            android:exported="false" />
        <activity
            android:name=".Splash"
            android:exported="true"
            android:theme="@style/Theme.TodayRestSub">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>